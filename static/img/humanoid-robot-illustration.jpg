<svg xmlns="http://www.w3.org/2000/svg" width="600" height="400" viewBox="0 0 600 400" style="background: #f8f9fa; border-radius: 8px;">
  <!-- Robot body -->
  <rect x="250" y="150" width="100" height="150" rx="10" fill="#e9ecef" stroke="#adb5bd" stroke-width="2"/>
  
  <!-- Robot head -->
  <circle cx="300" cy="120" r="40" fill="#e9ecef" stroke="#adb5bd" stroke-width="2"/>
  
  <!-- Robot eyes -->
  <circle cx="285" cy="110" r="5" fill="#495057"/>
  <circle cx="315" cy="110" r="5" fill="#495057"/>
  
  <!-- Robot mouth -->
  <path d="M 280 135 Q 300 145 320 135" stroke="#495057" stroke-width="2" fill="none"/>
  
  <!-- Robot arms -->
  <line x1="250" y1="170" x2="180" y2="200" stroke="#adb5bd" stroke-width="8" stroke-linecap="round"/>
  <line x1="250" y1="220" x2="180" y2="250" stroke="#adb5bd" stroke-width="8" stroke-linecap="round"/>
  <line x1="350" y1="170" x2="420" y2="200" stroke="#adb5bd" stroke-width="8" stroke-linecap="round"/>
  <line x1="350" y1="220" x2="420" y2="250" stroke="#adb5bd" stroke-width="8" stroke-linecap="round"/>
  
  <!-- Robot legs -->
  <line x1="270" y1="300" x2="270" y2="370" stroke="#adb5bd" stroke-width="8" stroke-linecap="round"/>
  <line x1="330" y1="300" x2="330" y2="370" stroke="#adb5bd" stroke-width="8" stroke-linecap="round"/>
  
  <!-- Decorative elements - representing sensors -->
  <circle cx="300" cy="120" r="8" fill="#20c997" opacity="0.7"/>
  <circle cx="270" cy="200" r="6" fill="#20c997" opacity="0.7"/>
  <circle cx="330" cy="200" r="6" fill="#20c997" opacity="0.7"/>
  
  <!-- Title text -->
  <text x="300" y="390" text-anchor="middle" font-family="Arial" font-size="24" fill="#495057">
    Physical AI & Humanoid Robotics
  </text>
</svg>